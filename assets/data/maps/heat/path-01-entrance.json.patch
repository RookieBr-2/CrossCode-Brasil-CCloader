    "307": {
      "settings": {
        "dialog": {
          "0": {
            "text": {
              "": "We finally made it!",
              "en_US": "Finalmente conseguimos!"
            }
          },
          "1": {
            "text": {
              "": "And after the freezing cold in \\v[area.bergen.name] the heat is very welcome.",
              "en_US": "E depois do frio congelante em \\v[area.bergen.name], o calor é muito bem-vindo."
            }
