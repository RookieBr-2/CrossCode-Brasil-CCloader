                "12": {
                  "message": {
                    "": "[nods]",
                    "en_US": "[acena a cabeça]"
                  }
                },
                "13": {
                  "message": {
                    "": "Bye!",
                    "en_US": "Tchau!"
                  }
                }
              }
            }
          },
