                "name": {
                  "": "McEdinthil"
                }
              },
              "2": {
                "message": {
                  "": "Greetings, \\v[lore.title.seeker], everything's running smoothly.",
                  "en_US": "Saudações, \\v[lore.title.seeker], tudo está funcionando perfeitamente."
