                },
                "2": {
                  "message": {
                    "": "Hi.",
                    "en_US": "Oi."
                  }
                },
                "3": {
                  "message": {
                    "": "\\v[lore.title.seeker].\\. A loyal soldier of the \\v[lore.title.human-government], I assume?",
                    "en_US": "\\v[lore.title.seeker].\\. Uma soldada leal das \\v[lore.title.human-government], eu presumo?"
