                    "fr_FR": "fr_FR",
                    "zh_CN": "\u4f46\u4e0d\u662f\u4eba\u4eba\u90fd\u80fd\u80dc\u4efb\u8b66\u5bdf\u7684\u5de5\u4f5c\uff0c\u4e0e\u793e\u4f1a\u7684\u9ed1\u6697\u9762\u505a\u5bf9\u6297\u7684\u3002\\.\u5c24\u5176\u662f\u4f60\u4eec\u8fd9\u6837\u7684\u8fc7\u5ba2\u3002",
                    "langUid": 443,
                    "ja_JP": "\u3060\u304c\u3001\u6d3e\u624b\u306a\u80a9\u66f8\u3092\u6301\u3063\u3066\u308b\u304b\u3089\u3068\u8a00\u3063\u3066\u8b66\u5099\u968a\u306e\n\u4ed5\u4e8b\u3092\u771f\u306b\u3053\u306a\u305b\u308b\u3068\u601d\u3046\u304b\uff1f\u51f6\u60aa\u306a\u72af\u4eba\u3068\n\u5bfe\u5cd9\u3067\u304d\u308b\u304b\uff1f\\.\u7591\u308f\u3057\u3044\u3082\u306e\u3060\u3002",
