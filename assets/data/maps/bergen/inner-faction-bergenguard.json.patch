                        "fr_FR": "fr_FR",
                        "zh_CN": "\u6211\u77e5\u9053\uff0c\u4f60\u4ee5\u4e3a\u8fd9\u6d3b\u5f88\u5bb9\u6613\uff1f \u5b83\u901f\u5ea6\u5f88\u5feb\uff0c\u5e76\u4e14\u559c\u6b22\u722c\u5230\u9ad8\u5904\u3002",
                        "langUid": 448,
                        "ja_JP": "\u3042\u3042\u3001\u308f\u304b\u308b\u3055\u3001\u6355\u307e\u3048\u308b\u3060\u3051\u306e\u5358\u7d14\u306a\u4ed5\u4e8b\u306b\n\u805e\u3053\u3048\u308b\u3060\u308d\u3046\u3002\u3060\u304c\u3001\u3084\u3064\u306f\u9ad8\u3044\u3068\u3053\u308d\u306b\n\u3068\u3066\u3082\u7d20\u65e9\u304f\u767b\u3063\u3066\u3057\u307e\u3046\u3093\u3060\u3002",
