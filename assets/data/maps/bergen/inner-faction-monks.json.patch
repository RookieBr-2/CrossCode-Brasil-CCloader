                    "fr_FR": "fr_FR",
                    "langUid": 379,
                    "zh_CN": "\u73b0\u5728\u6211\u60f3\u4f60\u6e34\u671b\u6211\u4eec\u7684\u5f15\u5bfc\uff0c\u5e76\u4e3a\u667a\u6167\u800c\u6765\u3002<<A<<[CHANGED 2018/09/19]",
