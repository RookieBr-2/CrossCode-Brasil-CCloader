                "2": {
                  "message": {
                    "": "Hi.",
                    "en_US": "Oi."
                  }
                },
                "3": {
                  "message": {
                    "": "Hey \\v[lore.title.seeker]!\\. Can I help you with anything?",
                    "en_US": "Ei \\v[lore.title.seeker]!\\. Posso te ajudar em alguma coisa?"
