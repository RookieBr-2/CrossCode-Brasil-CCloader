              "10": {
                "message": {
                  "": "[nods]",
                  "en_US": "[acena a cabeça]"
                }
              },
              "11": {
                "message": {
                  "": "Nice! So how do you trade it?",
                  "en_US": "Legal! Então, como fazemos a troca?"
                }
              },
              "12": {
                "message": {
                  "": "You don't.\\. They don't allow trading of cosmetics.",
                  "en_US": "Você não pode.\\. Eles não permitem troca de itens cosméticos."
