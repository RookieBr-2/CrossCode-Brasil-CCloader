    "371": {
      "settings": {
        "event": {
          "14": {
            "message": {
              "": "Here we are! That famous dungeon crossing!",
              "en_US": "Chegamos! Na famosa travessia de dungeon!"
            }
          },
          "22": {
            "message": {
              "": "So... which dungeon is first?",
              "en_US": "Então... qual dungeon é a primeira?"
            }
          },
          "23": {
            "message": {
              "": "I think that is up to us.",
              "en_US": "Eu acho que nós devemos escolher."
            }
          },
          "24": {
            "message": {
              "": "There are two temples we have to complete first in any order.",
              "en_US": "Existem dois templos que temos que completar primeiro em qualquer ordem."
            }
          },
          "31": {
            "message": {
              "": "The \\c[3]So'najiz Temple\\c[0] in the west...",
              "en_US": "O \\c[3]Templo de So'najiz\\c[0] no oeste"
            }
          },
          "38": {
