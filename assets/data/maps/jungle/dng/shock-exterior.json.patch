    "459": {
      "settings": {
        "event": {
          "8": {
            "message": {
              "": "Here we are... the Zir'vitar dungeon.",
              "en_US": "Aqui estamos... a dungeon de Zir'vitar."
            }
          },
          "17": {
            "message": {
              "": "Such a gloomy atmosphere...",
              "en_US": "Que atmosfera sombria..."
            }
