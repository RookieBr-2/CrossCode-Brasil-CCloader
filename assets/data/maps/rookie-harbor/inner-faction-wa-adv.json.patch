              }
            }
          },
          "2": {
            "event": {
              "quest": {
                "1": {
                  "name": {
                    "": "Elenya"
                  }
                },
                "2": {
                  "message": {
                    "": "Heya \\v[lore.title.seeker].\\. Did you come back to help us out with our problem?",
                    "en_US": "Ei \\v[lore.title.seeker].\\. Você voltou para nos ajudar com nosso problema?"
