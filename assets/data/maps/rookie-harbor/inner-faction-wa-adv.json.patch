          "6": {
            "event": {
              "1": {
                "name": {
                  "": "Elenya"
                }
              },
              "2": {
                "message": {
                  "": "Tucos should be waiting for you at \\c[3]Pathway 5\\c[0] in \\c[3]\\v[area.autumn-area.name]\\c[0].",
                  "en_US": "Tucos deve estar esperando por você na \\c[3]Estrada 5\\c[0] no \\c[3]\\v[area.autumn-area.name]\\c[0]."
