              "quest": {
                "1": {
                  "name": {
                    "": "Elenya"
                  }
                },
                "2": {
                  "message": {
                    "": "Welcome back.\\. I see retrieving the data went off without a hitch?",
                    "en_US": "Oi linda.\\. A recuperação dos dados foi tranquila?"
