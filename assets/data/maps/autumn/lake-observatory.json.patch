          "87": {
            "message": {
              "": "[shakes head]",
              "en_US": "[balança a cabeça]"
            }
          },
          "88": {
            "message": {
              "": "It seems like a long talk is due.",
              "en_US": "Parece que devemos ter uma longa conversa."
