              }
            },
            "10": {
              "message": {
                "": "[nods]",
                "en_US": "[acena a cabeça]"
              }
            },
            "11": {
              "message": {
                "": "Fine!\\. I'll write the scroll with the location.\\. Meet me there!",
                "en_US": "Tá bom!\\. Eu vou escrever o pergaminho indicando o local.\\. Me encontre lá!"
