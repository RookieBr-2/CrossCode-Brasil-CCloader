            },
            "50": {
              "name": {
                "": "Sao"
              }
            },
            "52": {
              "message": {
                "": "Yes, yes, the duel, fine!\\. I'll give you the scroll! ",
                "en_US": "Arre, sim, o duelo, entendi!\\. Eu vou te dar o pergaminho!"
              }
            },
            "53": {
              "message": {
                "": "God, why are you talking so much anyway?\\. I thought you couldn't do that!",
                "en_US": "Arre, por que você tá falando tanto assim?\\. Eu pensei que você não conseguia!"
              }
            }
          }
