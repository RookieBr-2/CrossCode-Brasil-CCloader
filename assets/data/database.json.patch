    "ctron-pandas-end": {
      "event": {
        "0": {
          "thenStep": {
            "0": {
              "message": {
                "": "Pandas...\\. Pandas are interesting.\\. Originally carnivores, they essentially evolved into bamboo eating machines.",
                "en_US": "Pandas...\\. Os Pandas são interessantes. Originalmente carnívoros, eles evoluíram essencialmente para máquinas comedoras de bambu."
